  "version": "0.0.1",
  "homepage": "https://github.com/semireg/win-rw",
  "author": {
    "name": "Caylan Larson",
    "url": "https://semireg.com/",
    "email": "caylan@semireg.com"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/semireg/win-rw.git"
  },
  "devDependencies": {
    "nodeunit": "*",
    "prebuild": "^9.1.1",
    "prebuild-ci": "^3.1.0"
